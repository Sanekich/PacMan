#include <iostream>
using namespace std;

char PacMan = 'O';
char map1[31][32];

void CreateMap()
{
    char map1Proto[31][32] = { 
    "############################",
    "#............##............#",
    "#.####.#####.##.#####.####.#",
    "#o####.#####.##.#####.####o#",
    "#.####.#####.##.#####.####.#",
    "#..........................#",
    "#.####.##.########.##.####.#", 
    "#.####.##.########.##.####.#",
    "#......##....##....##......#",
    "######.#####.##.#####.######",
    "     #.#####.##.#####.#     ",
    "     #.##          ##.#     ",
    "     #.## ###  ### ##.#     ",
    "######.## #      # ##.######",
    "      .   #      #   .      ",
    "######.## #      # ##.######",
    "     #.## ######## ##.#     ",
    "     #.##          ##.#     ",
    "     #.## ######## ##.#     ",
    "######.## ######## ##.######",
    "#............##............#",
    "#.####.#####.##.#####.####.#",
    "#.####.#####.##.#####.####.#",
    "#o..##....... O.......##..o#",
    "###.##.##.########.##.##.###",
    "###.##.##.########.##.##.###",
    "#......##....##....##......#",
    "#.##########.##.##########.#",
    "#.##########.##.##########.#",
    "#..........................#",
    "############################"
    };


    for(int i = 0;i < 31;i++)
    {
        for(int j = 0;j < 32;j++)
        {
            map1[i][j] = map1Proto[i][j];
        }

    }
    map1[27][15] == PacMan;
    for(int i = 0;i < 31;i++)
    {
        for(int j = 0;j < 32;j++)
        {
            cout << map1[i][j];
        }
        cout << endl;
    }
}

int main()
{
    CreateMap();
}
